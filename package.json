  "bugs": {
    "url": "https://github.com/tosinibrahim96/andela-developer-challenge/issues"
  },
  "homepage": "https://github.com/tosinibrahim96/andela-developer-challenge#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "joi": "^14.0.1",
    "jsonwebtoken": "^8.3.0",
    "make-runnable": "^1.3.6",
    "moment": "^2.22.2",
    "pg": "^7.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
